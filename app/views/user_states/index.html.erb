<p id="notice"><%= notice %></p>

<h1>User States</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Role</th>
      <th>District</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_states.each do |user_state| %>
      <tr>
        <td><%= user_state.user_id %></td>
        <td><%= user_state.start_date %></td>
        <td><%= user_state.end_date %></td>
        <td><%= user_state.role %></td>
        <td><%= user_state.district_id %></td>
        <td><%= link_to 'Show', user_state %></td>
        <td><%= link_to 'Edit', edit_user_state_path(user_state) %></td>
        <td><%= link_to 'Destroy', user_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User State', new_user_state_path %>
